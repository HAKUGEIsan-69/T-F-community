{
  "name": "hibarigaoka-track-communication",
  "version": "1.0.0",
  "description": "雲雀丘学園陸上部コミュニケーションツール",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["陸上部", "コミュニケーション", "学校"],
  "author": "雲雀丘学園陸上部",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "express-session": "^1.17.3",
    "bcrypt": "^5.1.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}